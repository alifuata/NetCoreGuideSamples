@model List<EmployeeModel>
@{

}



<table>
    <tr>
        <th>ID</th>
        <th>Name & Surname</th>
        <th>EMail</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.employeeId</td>
            <td>@item.nameSurname</td>
            <td>@item.eMail</td>
        </tr>
    }
</table>